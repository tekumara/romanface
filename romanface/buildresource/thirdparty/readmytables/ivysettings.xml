<ivysettings>
	<include url="${ivy.default.settings.dir}/ivysettings.xml"/>
	<resolvers>

		<url name="googlecode">
			<ivy pattern="http://[module].googlecode.com/svn/trunk/[module]/ivypublish/ivy-[revision].xml" />
			<artifact pattern="http://[module].googlecode.com/files/[artifact]-[revision].[ext]" />
		</url>

	</resolvers>
	<modules>
		<!--  modules not using default resolver -->
		<module organisation="org.omancode" resolver="googlecode" />
	</modules>
</ivysettings>
